{
    "manifest_version": 2,
    "name": "Translate konjac",
    "description": "This addon adds context menu link to Google translation.",
    "version": "1.0",
    "default_locale": "ja",
    "browser_specific_settings": {
        "gecko": {
            "strict_min_version": "56.0a1"
        }
    },

    "background": {
        "scripts": ["background.js"]
    },
  
    "permissions": [
        "menus",
        "tabs"
    ]
}
